  "paths": {

    "/consumer/reports/{requestId}/status": {
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "requestId",
          "in": "path",
          "required": true
        }
      ],
      "get": {
        "x-code-samples": [
          {
            "lang": "java",
            "source": "OkHttpClient client = new OkHttpClient();\r\n\r\nRequest request = new Request.Builder()\r\n  .url(\"https://api.cerebrumx.ai/v1/consumer/reports/requestId/status\")\r\n  .get()\r\n  .build();\r\n\r\nResponse response = client.newCall(request).execute();"
          },
          {
            "lang": "python",
            "source": "import http.client\r\n\r\nconn = http.client.HTTPSConnection(\"api.cerebrumx.ai\")\r\n\r\nconn.request(\"GET\", \"/v1/consumer/reports/requestId/status\")\r\n\r\nres = conn.getresponse()\r\ndata = res.read()\r\n\r\nprint(data.decode(\"utf-8\"))"
          }
        ],
        "summary": "Get Report Status",
        "tags": [
          "responses"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "reportStatus": {
                      "$ref": "#/components/schemas/ResponseStates"
                    },
                    "downloadUrl": {
                      "type": "string",
                      "description": "If response Status is 'GENERATED', it contains download URL of the Response Data else empty string."
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "getResponseStatus",
        "description": "Get Status of a Report",
        "x-internal": true
      }
    }

  },

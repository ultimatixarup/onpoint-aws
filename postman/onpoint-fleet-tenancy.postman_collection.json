{
  "info": {
    "name": "OnPoint Fleet Tenancy Management",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "description": "Fleet/tenant management APIs per requirements. Uses fleetTenancyBaseUrl and fleetTenancyApiKey from the environment."
  },
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "exec": [
          "const uuid = pm.variables.replaceIn('{{$guid}}');",
          "pm.variables.set('idempotencyKey', uuid);",
          "pm.variables.set('correlationId', uuid);",
          "const apiKey = pm.environment.get('fleetTenancyApiKey');",
          "if (apiKey) {",
          "  pm.variables.set('apiKey', apiKey);",
          "}",
          "const role = pm.environment.get('roleAdmin');",
          "const token = pm.environment.get('idToken');",
          "if (apiKey && !pm.request.headers.has('x-api-key')) {",
          "  pm.request.headers.add({ key: 'x-api-key', value: apiKey });",
          "}",
          "if (role && !pm.request.headers.has('x-role')) {",
          "  pm.request.headers.add({ key: 'x-role', value: role });",
          "}",
          "if (token && !pm.request.headers.has('Authorization')) {",
          "  pm.request.headers.add({ key: 'Authorization', value: `Bearer ${token}` });",
          "}"
        ]
      }
    }
  ],
  "item": [
    {
      "name": "Tenants",
      "item": [
        {
          "name": "Create tenant",
          "request": {
            "method": "POST",
            "header": [
              { "key": "x-api-key", "value": "{{apiKey}}" },
              { "key": "x-role", "value": "{{roleAdmin}}" },
              { "key": "Idempotency-Key", "value": "{{idempotencyKey}}" },
              { "key": "x-correlation-id", "value": "{{correlationId}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": "{{fleetTenancyBaseUrl}}/tenants",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"tenantId\": \"{{tenantId}}\",\n  \"name\": \"Acme Fleet\",\n  \"config\": {\n    \"retentionDays\": 90\n  },\n  \"reason\": \"initial setup\"\n}"
            }
          }
        },
        {
          "name": "Get tenant",
          "request": {
            "method": "GET",
            "header": [
              { "key": "x-api-key", "value": "{{apiKey}}" },
              { "key": "x-tenant-id", "value": "{{tenantId}}" }
            ],
            "url": "{{fleetTenancyBaseUrl}}/tenants/{{tenantId}}"
          }
        },
        {
          "name": "Patch tenant",
          "request": {
            "method": "PATCH",
            "header": [
              { "key": "x-api-key", "value": "{{apiKey}}" },
              { "key": "x-tenant-id", "value": "{{tenantId}}" },
              { "key": "x-role", "value": "{{roleTenantAdmin}}" },
              { "key": "Idempotency-Key", "value": "{{idempotencyKey}}" },
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": "{{fleetTenancyBaseUrl}}/tenants/{{tenantId}}",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Acme Fleet Updated\",\n  \"status\": \"ACTIVE\",\n  \"reason\": \"rename\"\n}"
            }
          }
        },
        {
          "name": "Suspend tenant",
          "request": {
            "method": "POST",
            "header": [
              { "key": "x-api-key", "value": "{{apiKey}}" },
              { "key": "x-role", "value": "{{roleTenantAdmin}}" },
              { "key": "x-tenant-id", "value": "{{tenantId}}" }
            ],
            "url": "{{fleetTenancyBaseUrl}}/tenants/{{tenantId}}:suspend"
          }
        },
        {
          "name": "Activate tenant",
          "request": {
            "method": "POST",
            "header": [
              { "key": "x-api-key", "value": "{{apiKey}}" },
              { "key": "x-role", "value": "{{roleTenantAdmin}}" },
              { "key": "x-tenant-id", "value": "{{tenantId}}" }
            ],
            "url": "{{fleetTenancyBaseUrl}}/tenants/{{tenantId}}:activate"
          }
        },
        {
          "name": "List tenants (admin)",
          "request": {
            "method": "GET",
            "header": [
              { "key": "x-api-key", "value": "{{apiKey}}" },
              { "key": "x-role", "value": "{{roleAdmin}}" }
            ],
            "url": "{{fleetTenancyBaseUrl}}/tenants"
          }
        }
      ]
    },
